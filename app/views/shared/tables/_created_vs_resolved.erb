<% if created_vs_resolved.present? && created_vs_resolved.count >0 %>
    <div class="CSSTableGenerator" >
      <table >
        <tr>
          <td>
            <%= t(:period) %>
          </td>
          <td >
            <%= t(:created) %>
          </td>
          <td >
            <%= t(:resolved) %>
          </td>
        </tr>
        <% created_vs_resolved.each do |t| %>
            <tr>
              <td>
                <%= t.period %>
              </td>
              <td >
                <%= t.unresolved %>
              </td>
              <td >
                <%= t.total_age %>
              </td>
            </tr>
        <% end %>
      </table>
    </div>
<% end %>